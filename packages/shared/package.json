{
  "name": "@speira/chordschart-shared",
  "version": "1.0.0",
  "author": "Speira",
  "keywords": [
    "utilities",
    "shared",
    "typescript"
  ],
  "license": "ISC",
  "description": "Shared utilities for chordschart",
  "type": "module",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "import": "./build/index.js"
    }
  },
  "scripts": {
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "build": "tsc"
  },
  "packageManager": "pnpm@10.14.0",
  "devDependencies": {
    "@effect/vitest": "^0.27.0",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "effect": "^3.19.11"
  },
  "effect": {
    "generateExports": {
      "include": [
        "**/*.ts"
      ]
    },
    "generateIndex": {
      "include": [
        "**/*.ts"
      ]
    }
  }
}
